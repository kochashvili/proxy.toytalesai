{
  "rewrites": [
    {
      "source": "/blog/",
      "destination": "https://blog-toytalesai.vercel.app/"
    },
    {
      "source": "/blog/:match*",
      "destination": "https://blog-toytalesai.vercel.app/"
    },
    { "source": "/", "destination": "https://toytalesai.com/" },
    {
      "source": "/:match*",
      "destination": "https://toytalesai.com/:match*"
    }
  ]
}
